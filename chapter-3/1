#include <stdio.h>
void escape(char old[],char new[]);
void main()
{
	char old[]='adsfdsfa\nsdafsdf\t';
	char new[100];
	escape(old,new);
}
void escape(char old[],char new[]){
	int i = 0;
	int j = 0;
	int n;
	while((n=old[i++])!='\0'){
		if(n=='\n'){
			new[j++]='\\';
			new[j++]='n';
		}else if(n=='\t'){
			new[j++]='\\';
			new[j++]='t';
		}else
			new[j]=n;
	}
	printf("%c\n",old);
	printf("%c\n",new);
}
